    <script
        type="text/javascript">Calendly.initBadgeWidget({ url: 'https://calendly.com/michael-imas/call15?hide_landing_page_details=1', text: `Schedule with<br/> Technician or Manager`, color: '#00a2ff', textColor: '#ffffff', branding: false });</script>
